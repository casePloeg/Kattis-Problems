
#include <iostream>
#include <cmath>

//09 - 03 - 2017
//Case Ploeg
using namespace std;

int main()
{
    string names;

    cin >> names;

    string capitals;

    for(int i=0; i<names.length(); i++){
        if(!isalpha(names[i])){
                capitals += names[i+1];
        }else if(i == 0){
            capitals += names[i];
        }
    }
     cout << capitals;
}
